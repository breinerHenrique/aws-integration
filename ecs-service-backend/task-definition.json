{
    "family": "intrack-task-nginx",
    "executionRoleArn": "arn:aws:iam::236730285067:role/ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512",
    "networkMode": "awsvpc",
    "requiresCompatibilities": ["FARGATE"],
    "containerDefinitions": [
      {
        "name": "intrack-container-nginx",
        "image": "PLACEHOLDER",
        "essential": true,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
              "awslogs-group": "ecs_web",
              "awslogs-region": "us-east-1",
              "awslogs-stream-prefix": "ecs"
          }
        },
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp",
            "name": "http"
          }
        ]
      }
    ]
  }